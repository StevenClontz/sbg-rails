<% @students.each do |student| %>
  <h3 style="page-break-before:always">
    Progress Report for <%=student.name%> &lt;<%=student.email%>&gt;
  </h3>
  <%= render 'students/progress', student: student, standard_categories: @course.standard_categories, grade_specifications: @course.grade_specifications, standards: @standards, editable: false %>
<% end %>
